<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>NServiceBus.ObjectBuilder.StructureMap</AssemblyName>
    <NuSpecFileName>NServiceBus.StructureMap</NuSpecFileName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <OutputPath>..\..\binaries\</OutputPath>
    <NoWarn>CS0419</NoWarn>
    <UpdateAssemblyInfo>true</UpdateAssemblyInfo>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="StructureMap" Version="4.4.2" />
    <PackageReference Include="Janitor.Fody" Version="1.5.1" />
    <PackageReference Include="NServiceBus" Version="7.0.0-alpharelease0051" />
    <PackageReference Include="Obsolete.Fody" Version="4.2.4" />
    <PackageReference Include="Particular.CodeRules" Version="0.2.0" />
    <PackageReference Include="NuGetPackager" Version="0.6.3" />
    <PackageReference Include="Fody" Version="2.1.2" />
    <PackageReference Include="GitVersionTask" Version="4.0.0-beta0012" />
    <None Include="..\..\GitVersion.yml" Link="GitVersion.yml" />
    <None Include="..\..\packaging\nuget\NServiceBus.StructureMap.nuspec" Link="NServiceBus.StructureMap.nuspec" />
  </ItemGroup>
</Project>